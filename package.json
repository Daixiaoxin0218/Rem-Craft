{
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "sass": "^1.53.0",
    "standard-version": "^9.5.0"
  },
  "version": "0.5.0",
  "description": "My theme for siyuan",
  "name": "rem-craft",
  "scripts": {
    "build": "sass --watch styles/index.scss:./theme.css -s compressed --no-source-map",
    "commit": "git add . && git cz && git push",
    "merge": "git checkout main && git merge dev",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
